{
  "name": "tas-backend",
  "version": "1.0.0",
  "private": true,
  "description": "TAS backend API (Express + TypeScript + Prisma)",
  "main": "dist/server.js",
  "scripts": {
    "dev": "node scripts/run-server.cjs dev",
    "build": "tsc -p tsconfig.json",
    "start": "node scripts/run-server.cjs start",
    "prisma:generate": "node scripts/run-prisma.cjs generate",
    "prisma:migrate": "node scripts/run-prisma.cjs migrate dev",
    "prisma:deploy": "node scripts/run-prisma.cjs migrate deploy",
    "db:seed": "node scripts/run-prisma.cjs db seed",
    "start:docker": "node scripts/start-docker.cjs",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.883.0",
    "@prisma/client": "^6.14.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.21.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "pdfkit": "^0.17.1",
    "pino": "^9.4.0",
    "pino-http": "^10.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.4",
    "@types/pdfkit": "^0.17.3",
    "@types/supertest": "^6.0.2",
    "pino-pretty": "^11.2.2",
    "prisma": "^6.14.0",
    "supertest": "^7.1.4",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
